{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run build --workspace=frontend",
  "outputDirectory": "/dist/",
  "rewrites": [
    {
      "source": "/generate-category",
      "destination": "/api/generate-category"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

